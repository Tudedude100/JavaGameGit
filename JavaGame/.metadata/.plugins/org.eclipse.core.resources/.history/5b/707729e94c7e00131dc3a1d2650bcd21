package GameState;

import java.awt.Graphics2D;

public class MenuState extends GameState {

	public MenuState(GameStateManager gsm) {
		
		this.gsm = gsm;
		
	}
	
	public void init() {}
	public void update() {}
	public void draw(Graphics2D g) {}
	public void keyPressed(int k) {}
	public void keyReleased(int k);

}
